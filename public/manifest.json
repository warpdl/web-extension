{
    "manifest_version": 3,
    "name": "WarpDL",
    "version": "0.1",
    "description": "Warp is a powerful and versatile cross-platform download manager.",
    "background": {
        "service_worker": "service_worker.js"
    },
    "permissions": [
        "downloads",
        "cookies"
    ],
    "host_permissions": [
        "https://*/*",
        "http://*/*"
    ]
}
  